  "author": "Paul Fernandez",
  "license": "Apache-2.0",
  "dependencies": {
    "d3-hierarchy": "^3.1.2",
    "graphology": "^0.26.0",
    "graphology-traversal": "^0.3.1",
    "gsap": "^3.14.2",
    "jolt-physics": "^1.0.0",
    "three": "^0.182.0",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/d3-hierarchy": "^3.1.7",
    "@types/node": "^24.0.0",
    "@types/three": "^0.182.0",
    "eslint": "^9.19.0",
    "eslint-plugin-jsdoc": "^50.6.0",
    "globals": "^15.14.0",
    "typescript": "^5.7.3",
    "vite": "^7.3.1"
