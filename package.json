{
  "name": "theory",
  "version": "0.2.0",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dyck": "node src/catalan/index.js --simulation=dyck",
    "dyck:tools": "node src/catalan/dyck-tools-cli.js",
    "dyck:center": "node src/catalan/index.js --center --simulation=dyck",
    "pairs": "node src/catalan/index.js --simulation=pairs",
    "pairs:center": "node src/catalan/index.js --center --simulation=pairs",
    "pairs:spine": "node src/catalan/index.js --simulation=pairs --spine",
    "pairs:spine:center": "node src/catalan/index.js --center --simulation=pairs --spine",
    "motzkin": "node src/catalan/index.js --simulation=motzkin",
    "motzkin:center": "node src/catalan/index.js --center --simulation=motzkin",
    "motzkin:spine": "node src/catalan/index.js --simulation=motzkin --spine",
    "motzkin:spine:center": "node src/catalan/index.js --center --simulation=motzkin --spine",
    "motifs": "node src/catalan/motif-discover-cli.js",
    "motifs:freeze": "node src/catalan/motif-discover-cli.js --freeze-balanced",
    "motifs:heavier": "node src/catalan/motif-discover-cli.js --policy=heavier",
    "motifs:lighter": "node src/catalan/motif-discover-cli.js --policy=lighter",
    "motifs:left": "node src/catalan/motif-discover-cli.js --policy=left",
    "motifs:right": "node src/catalan/motif-discover-cli.js --policy=right",
    "sk": "node src/cli/sk.js",
    "policies": "node src/catalan/policy-explorer-cli.js",
    "bijections": "node src/catalan/bijection-cli.js",
    "vis:dev": "vite",
    "vis:build": "vite build",
    "vis:preview": "vite preview",
    "test": "npm run typecheck && node --test tests/*.test.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "npm run lint && npm test"
